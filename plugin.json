{
	"id": "nodebb-plugin-twitter",
	"name": "NodeBB Twitter Embed Plugin",
	"description": "NodeBB Plugin that allows users to embed tweets inline in their posts.",
	"url": "https://github.com/dovydaskukalis/nodebb-plugin-twitter",
	"library": "./library.js",
	"hooks": [
	{ "hook": "filter:post.parse", "method": "parse", "callbacked": true, "priority": 6 },
	{ "hook": "filter:header.build", "method": "addNavigation", "callbacked": false },
	{ "hook": "filter:scripts.get", "method": "addScripts", "callbacked": false }
	],
	"staticDirs": {
		"public": "public"
	}
}
